function rotateArray(arr, k,n) {
    // const n = arr.length;
    const rotatedArr = [];
    for (let i = 0; i < n; i++) {
        rotatedArr[(i + k) % n] = arr[i];
    }
    return rotatedArr;
}


// const N = 5;
// const A = [1, 2, 3, 4, 5];
// const k = 1;


console.log(rotateArray([1,2,3,4,5],3,5));
