var a = "Mary";
var b = "army";

function isAnagram(firstString,secondString){
   var  a = firstString.toLowerCase();
   var  b = secondString.toLowerCase();

a = a.split("").sort().join("");
b = b.split("").sort().join("")
    if (a === b) {
        return "anagram"
    }
    else {
        return "Not"
    }
}

console.log(isAnagram(a,b))
